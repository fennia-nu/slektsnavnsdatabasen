{
  "name": "slektsnavn",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "prisma": {
    "seed": "bun run ./prisma/import-csv.ts"
  },
  "scripts": {
    "build": "remix vite:build",
    "deploy": "bun run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "bun run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "dump-db": "sqlite3 ./prisma/slektsnavn.db .dump > migrations/dump.sql",
    "d1-seed:local": "wrangler d1 execute slektsnavn --local --file=migrations/dump.sql",
    "d1-seed:remote": "wrangler d1 execute slektsnavn --remote --file=migrations/dump.sql",
    "d1-migrate:local": "wrangler d1 migrations apply slektsnavn --local",
    "d1-migrate:remote": "wrangler d1 migrations apply slektsnavn --remote"
  },
  "dependencies": {
    "@prisma/adapter-d1": "^5.20.0",
    "@prisma/client": "5.20.0",
    "@remix-run/cloudflare": "2.12.0",
    "@remix-run/cloudflare-pages": "2.12.0",
    "@remix-run/react": "2.12.0",
    "isbot": "^4.1.0",
    "prettier": "3.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@remix-run/dev": "2.12.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "postcss": "^8.4.38",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.78.12"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
